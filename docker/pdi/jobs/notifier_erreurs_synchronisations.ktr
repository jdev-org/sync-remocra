  <step>
    <name>Recupération erreurs par contexte</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>PG_EDP_SYNCHRO_REMOCRA</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT
	count(*) AS erreurs_nombre,
	array_to_string(ARRAY_AGG(err.id),',') AS erreurs_ids,
	'UPDATE edp.erreur SET notifie = true WHERE id IN(' ||array_to_string(ARRAY_AGG(err.id),',') ||')' AS sql,
	xmlelement(name "rapport",
		xmlelement(name "erreurs",
		xmlagg(
				xmlelement(  
					name erreur,
					xmlelement(name "dateHeureSynchronisation", to_char(err.date,'dd/mm/yyyy hh24:mi:ss')),
					xmlelement(name "refPEI", coalesce(tpei.reference,'-')),
					xmlelement(name "messageErreur", terr.message_erreur),
					xmlelement(name "messageAction", terr.message_action),
					xmlelement(name "donnees",
						xmlelement(name "contexteVisite", replace(json::json->>'contexte','NP', 'Changement d''état')),
						xmlelement(name "dateHeureVisite", to_char((json::json->>'date')::date,'dd/mm/yyyy à hh:mm:ss')),
						xmlelement(name "motifs", array_to_string(ARRAY(SELECT json_array_elements_text(json::json->'anomaliesConstatees')),', '))	   
					)
				)
			)
		)
	) AS xml_notification
FROM
	edp.erreur err
	JOIN edp.type_erreur terr ON (terr.id = err.type_erreur)
	LEFT JOIN edp.message mess ON (mess.id = err.message)
	LEFT JOIN edp.tracabilite_pei tpei ON (tpei.id = mess.id_traca_pei)
WHERE
	contexte = ?
AND
	NOT notifie</sql>
